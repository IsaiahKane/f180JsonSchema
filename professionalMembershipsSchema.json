{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://dev.interfolio.com/json-schemas/SCP-transfer.json",
	"title": "Professional Memberships Schema",
	"type": "object",
	"properties": {
		"start_term": {"type": "string"},
		"end_term": {"type": "string"},
		"organization": {"type": "string"},
		"offices_held": {"type": "string"},
		"description": {"type": "string"},
		"role": {
			"type": "string",
			"enum": [
				"Chair",
				"Co-Chair",
				"Coordinator",
				"Member"
			]
		},
		"scope": {
			"type": "string",
			"enum": [
				"Local",
				"State",
				"Regional",
				"National",
				"International"
			]
		}
	}
}