{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://dev.interfolio.com/json-schemas/SCP-transfer.json",
	"title": "Professioanl Licensures and Certifications Schema",
	"type": "object",
	"properties": {
		"faculty_id": {
			"type": "string"
		},
		"expiration_date": {
			"type": "string",
			"pattern": "^(19|20)\\d\\d([- /.])(0[1-9]|1[012])\\2(0[1-9]|[12][0-9]|3[01])$"
		},
		"license_number": {
			"type": "string"
		},
		"organization": {
			"type": "string"
		},
		"state": {
			"type": "string"
		},
		"status": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"type": {
			"type": "string"
		},
		"year_conferred": {
			"type": "number"
		},
		"recertification": {
			"type": "string"
		}
		"further_details": {
			"type": "string"
		}
	}
}
