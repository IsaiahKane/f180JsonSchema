{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://dev.interfolio.com/json-schemas/SCP-transfer.json",
	"title": "Military Experience Schema",
	"type": "object",
	"properties": {
		"status": {
			"type": "string",
			"enum": [
				"Inactive",
				"Active Duty",
				"Reserves",
				"Retired",
				"Inactive Reserves"
			]
		},
		"branch": {
			"type": "string",
			"enum": [
				"Air Force",
				"Air National Guard",
				"Army",
				"Coast Guard",
				"Marine Corps",
				"National Guard",
				"Navy"
			]
		},
		"start_date": {
			"type": "string",
			"pattern": "^(19|20)\\d\\d([- /.])(0[1-9]|1[012])\\2(0[1-9]|[12][0-9]|3[01])$"
		},
		"end_date": {
			"type": "string",
			"pattern": "^(19|20)\\d\\d([- /.])(0[1-9]|1[012])\\2(0[1-9]|[12][0-9]|3[01])$"
		},
		"carrer_narrative": {"type": "string"}
	}
}