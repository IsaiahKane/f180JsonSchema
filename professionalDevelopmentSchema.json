{
	"$schema": "http://json-schema.org/draft-06/schema#",
	"$id": "http://dev.interfolio.com/json-schemas/SCP-transfer.json",
	"title": "Professional Development Schema",
	"type": "object",
	"properties": {
		"faculty_id": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"start_term": {
			"type": "string"
		},
		"end_term": {
			"type": "string"
		},
		"organization": {
			"type": "string"
		},
		"city": {
			"type": "string"
		},
		"state": {
			"type": "string"
		},
		"hours_spent": {
			"type": ["string", "number"]
		},
		"description": {
			"type":" string"
		},
		"category": {
			"type": "string",
			"enum": [
				"Instructional",
				"Professional",
				"Research"
			]
		}
	}
}